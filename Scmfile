pipeline{
    agent any
 tools {
      maven 'maven3'
    }
    stages{
        stage("Git Checkout"){
            steps{
                git branch: 'main', credentialsId: 'github', url: 'https://github.com/Ragubharathi/project'
            }
        }
        stage("Maven Build"){
            steps{
                sh "mvn clean package"
        }      
    }
  } 
}
